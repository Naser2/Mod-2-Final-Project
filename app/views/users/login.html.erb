


  <%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :autocomplete => :off %>
</br>
  <%= f.label :username %>
  <%= f.text_field :username, :autocomplete => :off %>
</br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <h3> Do you want to sign up? <%= link_to "SignUp",  users_signup_path %></h>

  <%= f.submit %>
<% end %>
