<h1> Log in </h1>
<% if @user.errors.any? %>
<h2> Please correct the errors </h2>
  <ul>
    <% @user.errors.each do |msg| %>
    <li> <%= msg %></li>
  </ul>
  <% end  %>
<% end %>

  <%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  </br>
  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Login" %>

<% end %>

 <p> Sign Up? <%= link_to "SignUp", users_signup_path %></p>
